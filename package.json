{
  "name": "auth0-react-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel spa api-server",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "spa": "react-scripts start",
    "server": "node server.js",
    "server:dev": "nodemon server.js",
    "api-server": "node api-server.js",
    "api-server:dev": "nodemon api-server.js",
    "dev": "npm-run-all --parallel spa api-server:dev",
    "prod": "npm-run-all --parallel server api-server",
    "lint": "eslint --fix src/"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "cors": "^2.8.5",
    "expo": "^44.0.2",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "helmet": "^3.18.0",
    "highlight.js": "^10.0.3",
    "history": "^5.1.0",
    "install": "^0.13.0",
    "jest-fetch-mock": "^3.0.3",
    "jwks-rsa": "^1.4.0",
    "morgan": "^1.9.1",
    "npm": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
    "react-dom": "^16.8.6",
    "react-native": "0.66.4",
    "react-native-web": "^0.17.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "reactstrap": "^8.0.0",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.3.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.5.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
